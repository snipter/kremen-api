{
  "name": "@kremen/equipment-web",
  "version": "0.0.2",
  "title": "Комунальна техніка",
  "company": "#Kremen.Dev",
  "description": "Карта руху комунальної техніки, снігоприбиральників, посипальників та тракторів в Кременчуці",
  "url": "https://equipment.kremen.dev",
  "main": "src/index.tsx",
  "author": "Jaroslav Khorishchenko <websnipter@gmail.com>",
  "license": "ISC",
  "keywords": [
    "кременчук",
    "комунальна техніка",
    "карта",
    "мапа"
  ],
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --mode development --env.ENVFILE=.env.dev",
    "start:loc": "webpack-dev-server --mode development --env.ENVFILE=.env.loc",
    "dist": "yarn dist:prd",
    "dist:dev": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.dev",
    "dist:prd": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.prd",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "data:update": "./scripts/init_data_update.sh",
    "clear:cache": "rm -rf ./.cache",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "yarn clear:cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.ts,*tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kremen/types": "0.0.5",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "axios": "0.20.0",
    "color": "3.1.3",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-google-maps": "9.4.5",
    "react-motion": "0.5.2",
    "react-redux": "7.2.1",
    "react-responsive": "8.1.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/color": "3.0.1",
    "@types/googlemaps": "3.40.0",
    "@types/lodash": "4.14.162",
    "@types/markerclustererplus": "2.1.33",
    "@types/moment": "2.13.0",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-motion": "0.0.29",
    "@types/react-redux": "7.1.9",
    "@types/react-responsive": "8.0.2",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.2.1",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "eslint": "7.11.0",
    "eslint-plugin-react": "7.21.4",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "markdown-loader": "6.0.0",
    "prettier": "2.1.2",
    "raw-loader": "4.0.2",
    "redux-devtools-extension": "2.13.8",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.7",
    "typescript": "4.0.5",
    "url-loader": "4.1.1",
    "webpack": "5.3.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
